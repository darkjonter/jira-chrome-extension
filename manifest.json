{
	"name": "JIRA Issues viewer",
	"version": "1.0.17",
	"description": "With this extension you'll get simple access to all your issues or task from Atlassian  JIRA.",
	"background_page": "background.html",
	"icons": {
		"16": "images/16x16.png",
		"32": "images/32x32.png",
		"48": "images/48x48.png",
        "128": "images/128x128.png"
	},
	"permissions": [
		"tabs",
		"http://*/"
	],
   "options_page": "options.html",
	"browser_action": {
		"default_title": "JIRA issues viewer",
		"default_icon": "images/16x16.png",
		"popup": "jira.html"
	}
}