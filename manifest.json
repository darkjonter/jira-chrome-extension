{
	"name": "JIRA Chrome Extension",
	"version": "1.0.7",
	"description": "View your JIRA issues.",
	"background_page": "background.html",
	"icons": {
		"16": "images/16x16.png",
		"32": "images/32x32.png",
		"48": "images/48x48.png",
        "128": "images/128x128.png"
	},
	"permissions": [
		"tabs",
		"http://*/"
	],
   "options_page": "options.html",
	"browser_action": {
		"default_title": "Jira: Assigned to me",
		"default_icon": "images/16x16.png",
		"popup": "jira.html"
	}
}